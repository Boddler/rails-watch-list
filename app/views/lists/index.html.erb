<br>
<div class="container">
  <h1>All Lists</h1>
    <div>
      <div>
        <% @lists.each do |list|%>
          <div class="btn btn-primary">
            <%= link_to"#{list.name}", list_path(list), class: "link-unstyled color:white m-2"%>
          </div>
        <% end %>
      </div>
    </div>
  <%= link_to "Add List", new_list_path, class: "btn btn-dark mt-3, ml-5"%>
</div>
