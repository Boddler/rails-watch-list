<div class="container">
  <h1>All Lists</h1>
    <div class="cards">
      <% @lists.each do |list|%>
        <div>
          <div>
            <%= link_to"#{list.name}", list_path(list), class: "link-unstyled color:white"%>
            <br>
          </div>
        </div>
      <% end %>
    </div>
  <%= link_to "Add List", new_list_path, class: "btn btn-dark mt-3 mt-2"%>
</div>
